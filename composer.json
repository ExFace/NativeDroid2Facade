{
	"name" : "exface/NativeDroid2Template",
	"description" : "Material design UI template for the ExFace web ap plattform based on jQuery mobile and nativeDroid2",
	"type" : "library",
	"license" : "GPL-3.0+",
	"authors" : [{
			"name" : "Andrej Kabachnik",
			"email" : "kabachnik@gmail.com",
			"role" : "Creator"
		}
	],
	"support" : {
		"issues" : "https://github.com/exface/NativeDroid2Template/issues"
	},
	"require" : {
		"exface/core": "~0.1",
		"kabachello/jquery-nativedroid2": "dev-master", 
		"bower-asset/masonry" : "~4",
		"bower-asset/datatables.net" : "~1.10.12",
		"bower-asset/datatables.net-dt" : "~1.10.12",
		"npm-asset/screenfull" : "~3.0"
	},
    "autoload": {
        "psr-4": { "exface\\NativeDroid2Template\\": "" },
        "exclude-from-classmap": [
            "/Config/",
            "/Translations/",
            "/Model/"
        ]
    }
}